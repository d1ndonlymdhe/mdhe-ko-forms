generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Example {
  id String @id @default(cuid())
}

model User {
  id       String @id @default(cuid())
  username String
  password String
  salt     String
}

model userToken {
  id     String @id @default(cuid())
  userId String
  value  String
}
